<OysterRunScript>
    <Settings  RunScriptName="RefToStrAssertionRunScript" Explanation="On" Debug="On" ChangeReportDetail="Yes"  Trace="On" SS="Off" />
    
    <LogFile Num="5" Size="100000000">./RefToStrAssertion/Output/RefToStrAssertionLog_%g.log</LogFile>    
    <RunMode>AssertRefToStr</RunMode>

    <EREngine Type="FSCluster" /> 

    <AttributePath>./RefToStrAssertion/Scripts/RefToStrAssertionAttributes.xml</AttributePath>

    <IdentityInput Type="TextFile">./RefToStrAssertion/Input/RefToStrAssertionInputIdentities.idty</IdentityInput>

    <IdentityOutput Type="TextFile">./RefToStrAssertion/Output/RefToStrAssertionOutputIdentities.idty</IdentityOutput>

    <!-- Link Index is output but not used -->
    <LinkOutput Type="TextFile">./RefToStrAssertion/Output/RefToStrAssertionLinks.link</LinkOutput>
    
    <AssertionInput>./RefToStrAssertion/Scripts/RefToStrAssertionSourceDescriptor.xml</AssertionInput>
</OysterRunScript>
