<?xml version="1.0" encoding="UTF-8"?>

<!--
    Document   : ListOverlapRunScript.xml
    Created on : 2018-02-22
    Author     : J Talburt
    Description: Test of ListOverlap Comparator
-->

<OysterRunScript>
    <Settings RunScriptName="ListOverlapRunScript" Explanation="On" Debug="On" ChangeReportDetail="Yes"  Trace="On" SS="Off" />
    
    <LogFile Num="5" Size="100000000">./ListOverlap/Output/ListOverlapLog_%g.log</LogFile>  

    <RunMode>IdentityCapture</RunMode>

    <EREngine Type="FSCluster" />

    <!-- Attributes read from file only -->
    <AttributePath>./ListOverlap/Scripts/ListOverlapAttributes.xml</AttributePath>

    <!-- Identity-Capture does not start with any managed identities -->
    <IdentityInput Type="None"/>
    
    <!-- Identity-Capture produces a new identity file as output -->
    <IdentityOutput Type="TextFile">./ListOverlap/Output/ListOverlapOutput.idty</IdentityOutput>

    <!-- Identity-Capture creates the Link Index as output -->
    <LinkOutput Type="TextFile">./ListOverlap/Output/ListOverlapIndex.link</LinkOutput>
    
    <!-- Sources to Run -->
    <ReferenceSources>
        <Source>./ListOverlap/Scripts/ListOverlapSourceDescriptor.xml</Source>
    </ReferenceSources>
</OysterRunScript>
