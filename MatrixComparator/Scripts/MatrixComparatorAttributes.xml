<OysterAttributes System="School">
    <Attribute Item="StudentNameAddress"  Algo= "none" />
    <Attribute Item="LEA"  Algo="none" />
    <Attribute Item="StudentDOB"  Algo="none" />
          
    <!-- -->
<Indices>
	<Index Ident="X1">
		<Segment Item="StudentNameAddress" Hash="MatrixTokenizer(2, 'ST|AVE|BLVD')"/>
	</Index>
</Indices>

    <IdentityRules>
        <Rule Ident="1">
            <Term Item="StudentNameAddress" SIMILARITY="MatrixComparator(0.90,'ST|AVE|BLVD')"/> 
            <Term Item="StudentDOB" SIMILARITY="Exact"/>
        </Rule>
        <Rule Ident="2">
            <Term Item="StudentNameAddress" SIMILARITY="MatrixComparator(0.90,'ST|AVE|BLVD')"/> 
            <Term Item="LEA" SIMILARITY="Exact"/>
        </Rule>
    </IdentityRules>
</OysterAttributes>
